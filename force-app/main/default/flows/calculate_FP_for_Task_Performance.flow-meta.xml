    <formulas>
        <name>FP_MD</name>
        <dataType>Number</dataType>
        <expression>IF( ISNULL({!fitGrade}),0, 
Round( 
case( Text({!$Record.DevType__c}), &quot;목록&quot;,3.9, &quot;신규&quot;,4,&quot;수정&quot;,4,&quot;조회&quot;,3.9,&quot;삭제&quot;,4,&quot;보고서&quot;,5.2,&quot;대시보드&quot;,5.2, 0) /
case( {!fitGrade}, &quot;S-OOTB&quot;,3.8,&quot;Con-Light&quot;,3.4,&quot;Con-Heavy&quot;,3.2,&quot;Cus-Heavy&quot; ,1.2, 1) / 1.5, 1)
)</expression>
        <scale>2</scale>
