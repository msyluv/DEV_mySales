<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ScheduleMonth__c</fullName>
    <description>Month value of Schedule Date field</description>
    <externalId>false</externalId>
    <formula>TEXT(YEAR(ScheduleDate__c)) &amp; &quot;/&quot; &amp; IF(Month(ScheduleDate__c) &lt; 10, &quot;0&quot; &amp; TEXT(Month(ScheduleDate__c)), TEXT(Month(ScheduleDate__c)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Schedule Month</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
