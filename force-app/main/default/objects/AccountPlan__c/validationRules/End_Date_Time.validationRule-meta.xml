<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_Time</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT(ISNULL(EndDateTime__c)), StartDateTime__c &gt;=EndDateTime__c)</errorConditionFormula>
    <errorDisplayField>EndDateTime__c</errorDisplayField>
    <errorMessage>The end date must be greater than the start date.</errorMessage>
</ValidationRule>
